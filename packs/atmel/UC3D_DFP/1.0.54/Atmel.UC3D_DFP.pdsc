                             start="0xFFFF0000"
                             size="0x00007000"
                             type="other"
                             address-space="base"/>
                  <at:memory name="HTOP1"
                             start="0xFFFE0000"
                             size="0x00002400"
                             type="other"
                             address-space="base"/>
