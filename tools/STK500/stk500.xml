	<Connection type="Serial" name="com.atmel.avrdbg.connection.serialport" />
	<DESCRIPTION>The Atmel AVR STK500 is a starter kit and development system for the AVR flash microcontrollers from ATMEL Corporation.</DESCRIPTION>
	<WEBPAGE>http://www.microchip.com/Developmenttools/ProductDetails.aspx?PartNO=ATATMEL-ICE</WEBPAGE>
	<WEBSHOP></WEBSHOP>
	<ASOGUI>
		<IMAGEINDEX>8</IMAGEINDEX>
	</ASOGUI>
	<PROGRAMMINGSUPPORT>true</PROGRAMMINGSUPPORT>
	<PROGRAMMINGINTERFACES>
