				<InterfaceType>isp</InterfaceType>
			</InterfaceTypes>
			<CLOCK Name="ISP Clock" ClockType="Discrete">
				<Unit>Hz</Unit>
				<MINFREQUENCY>1206</MINFREQUENCY>
				<MAXFREQUENCY>1843200</MAXFREQUENCY>
				<DEFAULTFREQUENCY>125000</DEFAULTFREQUENCY>
