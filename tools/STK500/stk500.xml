					<FREQUENCY>1843200</FREQUENCY>
				</FREQUENCIES>
			</CLOCK>
		</INTERFACE>
		<INTERFACE DisplayName="HVPP" InterfaceName="HVPP">
			<InterfaceTypes>
				<InterfaceType>hvpp</InterfaceType>
			</InterfaceTypes>
		</INTERFACE>
		<INTERFACE DisplayName="HVSP" InterfaceName="HVSP">
			<InterfaceTypes>
				<InterfaceType>hvsp</InterfaceType>
			</InterfaceTypes>
		</INTERFACE>
	</PROGRAMMINGINTERFACES>
	<SupportsExternalResetOnStartSession>false</SupportsExternalResetOnStartSession>
